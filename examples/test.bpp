module Graphics {
    enum Color {
        RED
        GREEN
        BLUE
        BLACK
        GRAY
    }
    fn PrintColor(col) {
        print(Graphics.Color[col]);
    }
}

using Graphics

PrintColor(Color.BLUE);

match (Color.RED) {
    (Color.RED) {
        print("RED is RED");
    }
    (Color.GREEN) {
        print("RED is GREEN");
    }
}

